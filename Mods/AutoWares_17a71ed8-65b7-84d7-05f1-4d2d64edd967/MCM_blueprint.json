{
    "SchemaVersion": 1,
    "ModName": "Auto Wares",
    "Optional": true,
    "Tabs": [
        {
            "TabName": "Save&Load",
            "TabId": "AW_sw",
            "Settings": [
                {
                    "Id": "AW_sw_action",
                    "Name": "Action",
                    "Type": "radio",
                    "Default": "Merge",
                    "Options": {
                        "Choices": [
                            "Merge",
                            "Load",
                            "Save"
                        ]
                    },
                    "Tooltip": "Select an action"
                },
                {
                    "Id": "AW_sw_slot",
                    "Name": "Slot",
                    "Type": "slider_int",
                    "Default": 0,
                    "Options": {
                        "Min": 0,
                        "Max": 100
                    },
                    "Tooltip": "Select a slot"
                }
            ]
        },
        {
            "TabName": "Presets",
            "TabId": "AW_presets"
        },
        {
            "TabName": "Misc",
            "TabId": "AW_mics",
            "Settings": [
                {
                    "Id":"AW_enable",
                    "Name": "Enable",
                    "Type": "checkbox",
                    "Default": true,
                    "Tooltip": "Enable or disable ware traction"
                },
                {
                    "Id":"AW_show_notification",
                    "Name": "Show Notification",
                    "Type": "checkbox",
                    "Default": true,
                    "Tooltip": "Show Notification when adding to a new item to the chest"
                }
            ]
        }
    ]
}